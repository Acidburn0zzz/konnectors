{
  "name": "konnectors",
  "version": "0.2.1",
  "engines": [
    "node = 0.8.x"
  ],
  "description": "Store data from the web in your Cozy",
  "license": "AGPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/frankrousseau/konnectors.git"
  },
  "main": "server.coffee",
  "dependencies": {
    "americano": "0.3.3",
    "americano-cozy": "0.2.2",
    "printit": "0.1.3",
    "querystring": "0.2.0",
    "request-json": "0.4.10",
    "moment": "2.4.0",
    "request": "~2.30.0",
    "cozy-realtime-adapter": "~0.12.0",
    "cheerio": "0.16.0"
  },
  "devDependencies": {
    "nock": "0.27.0",
    "should": "2.0.1",
    "mocha": "1.13.0",
    "cozy-fixtures": "1.0.1",
    "coffeelint": "0.6.1"
  },
  "scripts": {
    "test": "cake tests",
    "start": "coffee server.coffee"
  },
  "cozy-permissions": {
    "Konnector": {
      "description": "Konnector description and required fields"
    },
    "RescueTimeActivity": {
      "description": "Activity data from RescueTime.com."
    },
    "JawboneMove": {
      "description": "Move data from jawbone.com."
    },
    "JawboneSleep": {
      "description": "Sleep data from jawbone.com."
    },
    "TwitterTweet": {
      "description": "Tweets you published on Twitter."
    },
    "PhoneBill": {
      "description": "Phone bills fetched by B&You Konnector."
    },
    "InternetBill": {
      "description": "Internet bills fetched by Free Konnector."
    },
    "File": {
      "description": "To import CSV or PDF file in the Cozy File System."
    },
    "WithingsScaleMeasure": {
      "description": "Import weight infos from Smart Body Analyzer."
    },
    "WithingsHeartBeat": {
      "description": "Import heartbeat from Smart Body Analyzer."
    }
  },
  "cozy-displayName": "Konnectors"
}
